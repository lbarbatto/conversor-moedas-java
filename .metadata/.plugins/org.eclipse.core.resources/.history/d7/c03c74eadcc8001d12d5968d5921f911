package conversororacle.view;

import java.util.ArrayList;

import conversororacle.model.Moedas;

public class MenuMoedas {
	
	public static ArrayList<String> display;
	
	public void listaMoedas() {
		
		for (Moedas lista : Moedas.values()) {
		      display.add(lista.getDescricao());
		  }
		System.out.println(display);
		
	}

}
